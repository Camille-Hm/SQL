-- Create a vintage clothes database


CREATE TABLE vintage_clothes (id integer primary key, name text, aisle integer, size integer, price integer, stock integer);

INSERT INTO vintage_clothes VALUES
(1, "Pants", 1, "S", 15, 10),
(2, "Shirts", 2, "S", 20, 12),
(3, "Dresses", 3, "S", 18, 6),
(4, "Skirts", 4, "S", 25, 4),
(5, "Cigarettes_pants", 5, "S", 30, 5);

--display the database ordered by price. 
SELECT * FROM vintage_clothes ORDER BY price asc;
